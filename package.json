    "ably": "^1.0.21",
    "antd": "^3.12.3",
    "babel-plugin-import": "^1.11.0",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "immutable": "^4.0.0-rc.12",
    "morgan": "^1.9.1",
    "nuclear-js": "^1.4.0",
    "prop-types": "^15.6.2",
